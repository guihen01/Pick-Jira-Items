<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>PickJiraItems</RootNamespace>
    <Authors>Henri Guillot</Authors>
    <Company>Henri Guillot</Company>
    <Description>Returns a list of JIRA, users, groups, name, emailadress matching a specific string
            //Kind of regex matching and grep search  JIRA users &amp; groups 
Based on REST API JIRA</Description>
    <PackageProjectUrl>https://github.com/guihen01/Pick-Jira-Items</PackageProjectUrl>
    <RepositoryUrl>https://github.com/guihen01</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>REST API JIRA Json Postman</PackageTags>
    <PackageId>PickJiraItems</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
